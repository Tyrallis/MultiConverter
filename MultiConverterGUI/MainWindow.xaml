<Window x:Class="MultiConverterGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MultiConverterGUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="405" Width="365">
    <Grid>
        <ListBox x:Name="lb" Margin="10,10,10,181" Drop="lb_Drop" DragEnter="lb_DragEnter" AllowDrop="true"/>
        <ProgressBar HorizontalAlignment="Center" Height="34" Margin="0,213,0,0" VerticalAlignment="Top" Width="345"/>
        <Button x:Name="btnFix" Content="Fix" HorizontalAlignment="Left" Margin="10,252,0,0" VerticalAlignment="Top" Height="42" Width="172" Click="btnFix_Click"/>
        <Button x:Name="btnClear" Content="Clear" HorizontalAlignment="Left" Margin="187,252,0,0" VerticalAlignment="Top" Height="42" Width="168" Click="btnClear_Click"/>

    </Grid>
</Window>
